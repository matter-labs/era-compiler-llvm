# Test how we are propagating conditional register reloads.

--- |
  source_filename = "test.ll"
  target datalayout = "E-p:256:256-i256:256:256-S256-a:256:256"
  target triple = "evm"

  define i256 @test(i256 %arg1, i256 %arg2, i256 %arg3) {
  entry:
    %cmp1 = icmp sgt i256 %arg1, 0
    %cmp2 = icmp sgt i256 %arg3, 0
    %zext = zext i1 %cmp2 to i256
    %sub = sub nuw nsw i256 %arg2, %zext
    br i1 %cmp1, label %bb1, label %bb2

  bb1:                                              ; preds = %entry
    %cmp3 = icmp sgt i256 %arg2, 0
    br i1 %cmp3, label %bb2, label %bb3

  bb2:                                              ; preds = %bb1, %entry
    br i1 %cmp2, label %bb3, label %bb4

  bb3:                                              ; preds = %bb2, %bb1
    %add1 = add nuw nsw i256 %arg1, 10
    br label %bb4

  bb4:                                              ; preds = %bb3, %bb2
    %phi1 = phi i256 [ %add1, %bb3 ], [ %sub, %bb2 ]
    ret i256 %phi1
  }

...
