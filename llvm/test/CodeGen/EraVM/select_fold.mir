    $r2 = ADDirr_s i256 1024, $r1, 0
    $r1 = ADDrrr_s killed $r2, $r0, 9, implicit $flags
    RET
...
---
# CHECK-LABEL: test_add_stack
# CHECK-LABEL: bb.0:
# CHECK: $r1 = ADDsrr_s i256 0, $r3, 0, $r1, 9
name: test_add_stack
tracksRegLiveness: true
body:             |
  bb.0:
    liveins: $r1, $r2, $r3
    $r2 = ADDsrr_s i256 0, $r3, 0, $r1, 0
    $r1 = ADDrrr_s killed $r2, $r0, 9, implicit $flags
    RET
...

