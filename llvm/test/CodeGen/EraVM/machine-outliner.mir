# CHECK-NEXT: $r1 = ORrrr_s $r1, $r2, 0
# CHECK-NEXT: $r1 = ORrrr_s $r1, $r2, 0
# CHECK-NEXT: $r1 = ORrrr_s $r1, $r2, 0
# CHECK-NEXT: $r1 = ORrrr_s $r1, $r2, 0
# CHECK-NEXT: $r1 = ORrrr_s $r1, $r2, 0
